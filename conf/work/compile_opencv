cmake -D CMAKE_INSTALL_PREFIX=install -D WITH_IPP=ON -D WITH_QT=OFF -D WITH_OPENGL=OFF -D BUILD_opencv_world=ON -D ENABLE_NEON=ON -D ENABEL_VFPV3=ON -D BUILD_TESTS=OFF -D INSTALL_PYTHON_EXAMPLES=OFF -D BUILD_EXAMPLES=OFF -D CMAKE_TOOLCHAIN_FILE=../platforms/linux/arm-gnueabi.toolchain.cmake -D WITH_TBB=ON -D BUILD_TBB=ON ..

cmake -D CMAKE_INSTALL_PREFIX=install -D WITH_IPP=ON -D WITH_QT=OFF -D WITH_OPENGL=OFF -D BUILD_opencv_world=ON -D ENABLE_NEON=ON -D ENABEL_VFPV3=ON -D BUILD_TESTS=OFF -D INSTALL_PYTHON_EXAMPLES=OFF -D BUILD_EXAMPLES=OFF   -D CMAKE_TOOLCHAIN_FILE=../platforms/linux/arm-gnueabi.toolchain.cmake -D BUILD_SHARED_LIBS=ON ..

cmake -D CMAKE_INSTALL_PREFIX=install -D WITH_IPP=ON -D WITH_QT=OFF -D WITH_OPENGL=OFF -D BUILD_opencv_world=ON -D ENABLE_NEON=ON -D ENABEL_VFPV3=ON -D BUILD_TESTS=OFF -D INSTALL_PYTHON_EXAMPLES=OFF -D BUILD_EXAMPLES=OFF   -D WITH_IPP=OFF  -D CMAKE_TOOLCHAIN_FILE=../platforms/linux/arm-gnueabi.toolchain.cmake -D BUILD_SHARED_LIBS=ON -D BUILD_opencv_python2=OFF -D BUILD_opencv_python3=OFF ..
	注意cmake之后修改CMAKE_AR-NOTFOUND 以及其他notfround为你的路径
